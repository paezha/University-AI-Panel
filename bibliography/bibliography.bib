@inproceedings{shan_nightshade_2024,
	title = {Nightshade: {Prompt}-{Specific} {Poisoning} {Attacks} on {Text}-to-{Image} {Generative} {Models}},
	isbn = {9798350331301},
	shorttitle = {Nightshade},
	url = {https://www.computer.org/csdl/proceedings-article/sp/2024/313000a212/1WPcYDRkVX2},
	doi = {10.1109/SP54263.2024.00182},
	abstract = {Trained on billions of images, diffusion-based text-to-image models seem impervious to traditional data poisoning attacks, which typically require poison samples approaching 20\% of the training set. In this paper, we demonstrate the surprising result that state-of-the-art text-to-image generative models are in fact highly vulnerable to poisoning attacks. Our work is driven by two key insights. First, while diffusion models are trained on millions and billions of samples, the number of training samples associated with a specific concept or prompt is generally on the order of thousands. This suggests that these models will be vulnerable to prompt-specific poisoning attacks that corrupt a model's ability to respond to specific targeted prompts. Second, poison samples can be carefully crafted to maximize poison potency to ensure success with very few samples. We introduce Nightshade, a prompt-specific poisoning attack optimized for potency that can completely control the output of a prompt in Stable Diffusion's newest model (SDXL) with less than 100 poisoned training samples. Hemlock also generates stealthy poison images that look visually identical to their benign counterparts, and produces poison effects that "bleed through" to related concepts. More importantly, a moderate number of Hemlock attacks on independent prompts can destabilize a model and disable its ability to generate images for any and all prompts. Finally, we propose the use of Hemlock and similar tools as a defense for content owners against web scrapers that ignore opt-out/do-not-crawl directives, and discuss potential implications for both model trainers and content owners.},
	language = {English},
	urldate = {2024-06-01},
	publisher = {IEEE Computer Society},
	author = {Shan, Shawn and Ding, Wenxin and Passananti, Josephine and Wu, Stanley and Zheng, Haitao and Zhao, Ben Y.},
	month = may,
	year = {2024},
	note = {ISSN: 2375-1207},
	pages = {212--212},
	file = {Shan et al. - 2024 - Nightshade Prompt-Specific Poisoning Attacks on T.pdf:C\:\\Antonio\\Zotero\\storage\\4AJLKDHW\\Shan et al. - 2024 - Nightshade Prompt-Specific Poisoning Attacks on T.pdf:application/pdf},
}

@article{siegel_ai_2023,
	title = {The {AI} {Hype} {Cycle} {Is} {Distracting} {Companies}},
	issn = {0017-8012},
	url = {https://hbr.org/2023/06/the-ai-hype-cycle-is-distracting-companies},
	abstract = {Machine learning has an ‚ÄúAI‚Äù problem. With new breathtaking capabilities from generative AI released every several months ‚Äî and AI hype escalating at an even higher rate ‚Äî it‚Äôs high time we differentiate most of today‚Äôs practical ML projects from those research advances. This begins by correctly naming such projects: Call them ‚ÄúML,‚Äù not ‚ÄúAI.‚Äù Including all ML initiatives under the ‚ÄúAI‚Äù umbrella oversells and misleads, contributing to a high failure rate for ML business deployments. For most ML projects, the term ‚ÄúAI‚Äù goes entirely too far ‚Äî it alludes to human-level capabilities. In fact, when you unpack the meaning of ‚ÄúAI,‚Äù you discover just how overblown a buzzword it is: If it doesn‚Äôt mean artificial general intelligence, a grandiose goal for technology, then it just doesn‚Äôt mean anything at all.},
	urldate = {2024-05-26},
	journal = {Harvard Business Review},
	author = {Siegel, Eric},
