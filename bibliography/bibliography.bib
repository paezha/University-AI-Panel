	copyright = {Copyright (c) 2024 First Monday},
	issn = {1396-0466},
	shorttitle = {The {TESCREAL} bundle},
	url = {https://firstmonday.org/ojs/index.php/fm/article/view/13636},
	doi = {10.5210/fm.v29i4.13636},
	abstract = {The stated goal of many organizations in the field of artificial intelligence (AI) is to develop artificial general intelligence (AGI), an imagined system with more intelligence than anything we have ever seen. Without seriously questioning whether such a system can and should be built, researchers are working to create ‚Äúsafe AGI‚Äù that is ‚Äúbeneficial for all of humanity.‚Äù We argue that, unlike systems with specific applications which can be evaluated following standard engineering principles, undefined systems like ‚ÄúAGI‚Äù cannot be appropriately tested for safety. Why, then, is building AGI often framed as an unquestioned goal in the field of AI? In this paper, we argue that the normative framework that motivates much of this goal is rooted in the Anglo-American eugenics tradition of the twentieth century. As a result, many of the very same discriminatory attitudes that animated eugenicists in the past (e.g., racism, xenophobia, classism, ableism, and sexism) remain widespread within the movement to build AGI, resulting in systems that harm marginalized groups and centralize power, while using the language of ‚Äúsafety‚Äù and ‚Äúbenefiting humanity‚Äù to evade accountability. We conclude by urging researchers to work on defined tasks for which we can develop safety protocols, rather than attempting to build a presumably all-knowing system such as AGI.},
	language = {en},
	urldate = {2024-05-31},
	journal = {First Monday},
	author = {Gebru, Timnit and Torres, √âmile P.},
	month = apr,
	year = {2024},
	file = {Full Text PDF:C\:\\Antonio\\Zotero\\storage\\AGFG5J7B\\Gebru and Torres - 2024 - The TESCREAL bundle Eugenics and the promise of u.pdf:application/pdf},
}

