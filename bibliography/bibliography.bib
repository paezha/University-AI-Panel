	year = {2023},
	note = {Conference Name: IEEE Access},
	keywords = {Adaptation models, Brazilian coins, classification, coin recognition, convolution neural network, Convolutional neural networks, Currencies, Deep learning, feature extraction, Feature extraction, Image edge detection, model evaluation, Training},
	pages = {109448--109461},
	file = {IEEE Xplore Full Text PDF:/home/antonio-paez/antonio-rogue/Zotero/storage/A8Z4Q924/Swain et al. - 2023 - A Deep Learning Framework for the Classification o.pdf:application/pdf},
}

