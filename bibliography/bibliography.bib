	keywords = {Chaotic secure multi-verse optimization algorithm, Man in the middle attack, Multi-verse optimization, Optimization, Routing, Security, VANET},
	file = {Full Text PDF:/home/antonio-paez/antonio-rogue/Zotero/storage/CHTIUD55/Sumit et al. - 2024 - A dynamic and optimized routing approach for VANET.pdf:application/pdf},
