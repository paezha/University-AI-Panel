@article{sumit_dynamic_2024,
	title = {A dynamic and optimized routing approach for {VANET} communication in smart cities to secure intelligent transportation system via a chaotic multi-verse optimization algorithm},
	issn = {1573-7543},
	url = {https://doi.org/10.1007/s10586-024-04322-9},
